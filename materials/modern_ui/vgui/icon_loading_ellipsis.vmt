UnlitGeneric
{
	$basetexture "modern_ui/vgui/icon_loading_ellipsis"
	$translucent 1
	$vertexcolor 1
	$vertexalpha 1
	$no_fullbright 1
	$ignorez 1
	$nolod 1

	Proxies
	{
		AnimatedTexture
		{ 
			animatedTextureVar $basetexture
			animatedTextureFrameNumVar $frame
			// this is the FPS that the texture will animate at
			animatedTextureFrameRate 60
		}
	}
}
